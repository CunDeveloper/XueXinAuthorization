<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name></display-name>
  <error-page>
    <error-code>404</error-code>
    <location>/NotFoundExceptionHandler</location>
  </error-page>
  <error-page>
    <exception-type>javax.servlet.ServletException</exception-type>
    <location>/ServletExceptionHandler</location>
  </error-page>
  <context-param>
    <param-name>image_upload_path</param-name>
    <param-value>upload_image_path</param-value>
  </context-param>
  <context-param>
    <param-name>image_upload_default_path</param-name>
    <param-value>default_path</param-value>
  </context-param>
  <context-param>
    <param-name>user</param-name>
    <param-value>user_icon</param-value>
  </context-param>
  <filter-mapping>
    <filter-name>SessionFilter</filter-name>
    <url-pattern></url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>CharSetFilter</filter-name>
    <url-pattern></url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>ValidateFilter</filter-name>
    <url-pattern></url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>test</servlet-class>
  </servlet>
  <servlet>
    <description> </description>
    <servlet-name>InitEnvService</servlet-name>
    <servlet-class>com.nju.manager.InitEnvService</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>log4j-init</servlet-name>
    <servlet-class>com.nju.manager.Log4jInit</servlet-class>
    <init-param>
      <param-name>log4j-init-file</param-name>
      <param-value>WEB-INF/classes/log4j.properties</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>ValidateController</servlet-name>
    <servlet-class>com.nju.validate.ValidateController</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <description> </description>
    <servlet-name>FileUploadController</servlet-name>
    <servlet-class>com.nju.control.FileUploadController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/servlet/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InitEnvService</servlet-name>
    <url-pattern>/InitEnvService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUploadController</servlet-name>
    <url-pattern>/FileUploadController</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>